{
  "name": "@swup/theme",
  "amdName": "SwupTheme",
  "version": "2.0.0",
  "description": "Base class for creating swup themes",
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.modern.js",
      "types": "./dist/index.d.ts"
    }
  },
  "unpkg": "./dist/index.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "swup package:build",
    "dev": "swup package:dev",
    "lint": "swup package:lint",
    "format": "swup package:format",
    "prepublishOnly": "npm run build"
  },
  "author": "Georgy Marchuk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/swup/theme.git"
  },
  "dependencies": {
    "@swup/plugin": "^4.0.0"
  },
  "devDependencies": {
    "@swup/cli": "^5.0.0"
  },
  "peerDependencies": {
    "swup": "^4.0.0"
  },
  "browserslist": [
    "extends @swup/browserslist-config"
  ],
  "prettier": "@swup/prettier-config"
}
